{
  "name": "file-commander-control",
  "version": "1.0.0",
  "description": "Made with create-react-library",
  "author": "uriegel",
  "license": "MIT",
  "repository": "uriegel/file-commander-control",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "copyfiles -f  typescript/styles.module.css src && tsc && microbundle-crl --no-compress --format modern,cjs",
    "start": "copyfiles -f  typescript/styles.module.css src && tsc && microbundle-crl watch --no-compress --format modern,cjs",
    "prepare": "run-s build",
    "predeploy": "cd example && npm install && npm run build"
  },
  "peerDependencies": {
    "react": "^17.0.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.2",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "microbundle-crl": "^0.13.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "^4.0.2"
  },
  "files": [
    "dist"
  ]
}
